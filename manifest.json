{
  "manifest_version": 3,
  "name": "Chrome Clock",
  "description": "Displays a digital clock on Chrome's new tab page",
  "version": "1.0",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "128": "images/icon-128.png"
  }
}
